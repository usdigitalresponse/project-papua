{
  "name": "resolver",
  "version": "0.0.1",
  "description": "Performs validation and CRUD operations on S3 objects",
  "main": "index.js",
  "scripts": {
    "local": "S3_ENDPOINT='http://localhost:4572' ACCT_ID=123456789 ts-node src/local.ts",
    "test": "jest"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "\\.test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "exec": "yarn local",
    "ext": "ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3-node": "^0.1.0-preview.2",
    "@hapi/joi": "^17.1.1",
    "aws-serverless-express": "^3.3.8",
    "body-parser": "^1.19.0",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "js-yaml": "colinking/js-yaml#block-style-option",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@types/aws-serverless-express": "^3.3.3",
    "@types/chai": "^4.2.11",
    "@types/email-validator": "^1.0.6",
    "@types/express": "^4.17.6",
    "@types/hapi__joi": "^16.0.12",
    "@types/jest": "^25.2.1",
    "@types/js-yaml": "^3.12.3",
    "@types/moment": "^2.13.0",
    "@types/node": "^13.11.1",
    "jest": "^25.4.0",
    "nodemon": "^2.0.3",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
}
